{
  "name": "@cgk-platform/onboarding",
  "version": "1.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@cgk-platform/auth": "workspace:*",
    "@cgk-platform/db": "workspace:*",
    "@cgk-platform/logging": "workspace:*"
  },
  "devDependencies": {
    "@cgk-platform/typescript-config": "workspace:*",
    "tsup": "^8.0.0",
    "typescript": "^5.9.0",
    "vitest": "^3.0.0"
  },
  "peerDependencies": {
    "typescript": "^5.9.0"
  }
}
